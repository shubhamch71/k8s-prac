kubectl config set-context dev-user-context \
  --cluster=minikube \
  --namespace=test \
  --user=dev-user \
  --kubeconfig=dev-user.kubeconfig

kubectl config use-context dev-user-context --kubeconfig=dev-user.kubeconfig
